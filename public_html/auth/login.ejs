<div class="row">
    <div class="container-fluid">
        <h2 class="text-center">로그인<span class="text-muted" style="font-size: 0.6em; font-weight: 300"> with Dimigoin Account</span></h2>
    </div>
</div>
<div class="row">
    <div class="container-fluid">
        <form method="post" action="/auth/login">
            <div class="form-group">
                <input type="text" class="form-control" name="id" tabindex="1" placeholder="아이디">
            </div>
            <div class="form-group">
                <input type="password" class="form-control" name="pwd" tabindex="2" placeholder="비밀번호">
            </div>
            <div class="form-group">
                <input type="submit" class="btn btn-block btn-login" value="로그인">
            </div>
        </form>
    </div>
</div>
